{
  "name": "fvtt-loot-sheet-npc-5e",
  "version": "3.6.0",
  "description": "A javascript FoundryVTT module. Adding an additional NPC sheet for Loot/Containers/Merchant",
  "main": "lootsheetnpc5e.js",
  "scripts": {
    "all": "npm run less && npm run copyfiles && npm run test",
    "build": "npm run less && rollup -c",
    "local-deploy": "npm run build && npm run localcopy",
    "localcopy": "cd dist && docker cp . foundryvtt_foundry_1:/data/Data/modules/lootsheetnpc5e",
    "less": "lessc -source-map ./src/styles/lootsheetNPC5e.less ./dist/styles/lootsheetNPC5e.css",
    "generate-docs": "jsdoc -c jsdoc.json",
    "test": "nyc --reporter=text mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jopeek/fvtt-loot-sheet-npc-5e.git"
  },
  "keywords": [
    "javascript",
    "less",
    "css",
    "FoundryVTT",
    "module"
  ],
  "author": "Jan Ole Peek (ChalkOne)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jopeek/fvtt-loot-sheet-npc-5e/issues"
  },
  "homepage": "https://github.com/jopeek/fvtt-loot-sheet-npc-5e#readme",
  "bundledDependencies": [
    "tippy.js"
  ],
  "types": "./types/index.d.ts",
  "devDependencies": {
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@league-of-foundry-developers/foundry-vtt-dnd5e-types": "^1.5.2-2",
    "@league-of-foundry-developers/foundry-vtt-types": "^9.269.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.3",
    "@wikimedia/less-plugin-clean-css": "^1.5.2",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.0",
    "less": "^4.1.2",
    "lint": "^1.1.2",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "recursive-copy-cli": "^1.0.20",
    "rollup": "^2.66.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "unit.js": "^2.1.1"
  },
  "type": "module",
  "dependencies": {
    "babel": "^6.23.0",
    "tippy.js": "^6.3.7",
    "typescript": "^4.7.4"
  },
  "bundleDependencies": [
    "tippy.js"
  ]
}
